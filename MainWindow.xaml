<Window x:Class="dotnet_installer_example.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:dotnet_installer_example"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Border Padding="20">
        <Grid>
            <Frame x:Name="Main" NavigationUIVisibility="Hidden" Margin="0,0,0,30"/>
            <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal" Height="30" HorizontalAlignment="Right">
                <Button x:Name="BackButton" Content="Back" Click="Button_Back_Click" MinWidth="100" />
                <Button x:Name="ForwardButton" Content="Forward" Click="Button_Forward_Click" MinWidth="100" Margin="20,0,0,0" />
            </StackPanel>
        </Grid>
    </Border>
</Window>
